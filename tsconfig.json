{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ES6",
    "allowJs": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": "./",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "declaration": true,
    "strictNullChecks": true,
    "paths": {
      "iSurfaces/*": [
        "src/iSurfaces/*"
      ],
      "*": [
        "node_modules/*"
      ]
    },
  },
  "include": [
    "src/**/*",
    "assets/schemas"
  ],
  "exclude": [
    "src/App_old.tsx"
  ],
  "files": [
    "src/preload.d.ts",
    "src/scss.d.ts",
  ]
}